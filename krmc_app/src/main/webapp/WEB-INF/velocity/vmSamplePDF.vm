<html>
<head>
<style type="text/css">
/* common css */
#headwater table {
	height: 5%;
	width: 630;
}


body {
	font-size: 9pt;
	line-height: 300%;
	font-weight: normal;
	font-variant: normal;
}

th {
	background-color: #f5f5f5;
	font-weight: bold;
}

td {
	font-size: 10pt;
	color: 000000;
	line-height: 17pt;
}


.tableround {
	border-top-width: 1;
	border-right-width: 1;
	border-bottom-width: 1;
	border-left-width: 1;
	border-color: 000000;
	border-style: solid;
}

.cell_data_00 {
	font-size: 9pt;
	padding-left: 2px;
	
	background-repeat: repeat;
	border-top-width: 1;
	border-right-width: 0.99;
	border-bottom-width: 0.99;
	border-left-width: 1;
	border-color: 000000;
	border-style: solid;
	line-height: 17px;
}
.cell_data_02 {
	font-size: 9pt;
	padding-left: 2px;
	
	background-repeat: repeat;
	border-top-width: 0.01;
	border-right-width: 0.99;
	border-bottom-width: 1;
	border-left-width: 1;
	border-color: 000000;
	border-style: solid;
	line-height: 14px;
}

.cell_data_01 {
	font-size: 9pt;
	padding-left: 2px;
	
	background-repeat: repeat;
	border-top-width: 0.01;
	border-right-width: 1;
	border-bottom-width: 1;
	border-left-width: 0.01;
	border-color: 000000;
	border-style: solid;
	line-height: 17px;
}


.cell_data_03 {
	font-size: 9pt !important;
	padding-left: 2px;
	
	background-repeat: repeat;
	border-top-width: 1;
	border-right-width: 1;
	border-bottom-width: 0.99;
	border-left-width: 0.01;
	border-color: 000000;
	border-style: solid;
	line-height: 17px;
}

.title{
	line-height: 50px;
	font-size:26;
}


.breakhere {
	page-break-after: always;
	width: 640;
	height: 10px;
}

/* Page css */
.tableRound td, .tableRound th {
	padding: 3px;
}
.reportType td {
	font-size: 10px;
}

.select {
	background-color: black;
	color: white;
}

.header td {
	font-size: 10px;
	line-height: 15px;
}

.tdReport table,.tdReport div{
}

.tdReport div table{
	margin : 5px;
}

.tb-efm th{
	width:20%;
}

/*결재라인*/
.table-paymentLine{
	width:100%;
	
}

.td-paymentLine1{
	height:80px;
	width:20px;
	text-align:center;
}
/*상단*/
.td-paymentLine2{
	height:20px;
	width:60px;
	text-align:center;
}
/*하단*/
.td-paymentLine3{
	height:60px;
	width:60px;
}
.table-mst{
	border:1px solid black;
	height:200%;
}
hr{
	margin: 0 !important;
}
/* 
   pdf변환시 주의사항
   colgroup 설정 불가, border 일부 에러, 일부 css 불가
   th 자동 굵게 안됨
*/
</style>
</head>
<body style="font-family: igothic;" >
<div id="content" align="center" >
	<table width="100%" height="100%" cellpadding="0" cellspacing="0" align="center" class="table-mst" style="">
		<tr>
			<td class="tdReport ">
				<table width="100%" cellpadding="0" cellspacing="0">
					<tbody>
						<tr>
							<td style="2%;">&nbsp;</td>
							<td height="80" width="" align="left" style="vertical-align:bottom;width:31.3%; padding-left:20px;display: inline;">
								<img height="60" src="$!webUrl/resources/images/common/lotteria_logo.png" />
							</td>
							<td height="80" width="" align="center" class="title" style="width:33.3%">
								REPORT<br/>
								<table width="100%" cellpadding="2" cellspacing="0" border="0" class="reportType">
									<tr>
										<td class="cell_data_00" align="center" width="50">DRAFT</td>
										<td class="cell_data_03 select" align="center" width="50">REPORT</td>
										<td class="cell_data_03" align="center" width="50">MEETING</td>
									</tr>
								</table>
							</td>
							<td style="width:33.3%;">
							</td>
						</tr>
					</tbody>
				</table>
				
				<hr></hr>

				<table width="100%" border="0" cellspacing="0" cellpadding="0" class="header">
					<tbody>
						<tr height="16">
							<td style="2%;" rowspan="5">&nbsp;</td>
						
							<td style="width:6.3%;">&nbsp;FILE NO</td>
							<td style="width:58%;">$!data.comCd</td>
							<td rowspan="5" style="width:33.3%;">
								<table border="0" cellspacing="0" cellpadding="0" class="tableRound table-paymentLine">
									<tr>
										<td width="40" rowspan="2"></td>
										<td class="cell_data_00 td-paymentLine1" rowspan="2">D<br/>E<br/>C<br/>I</td>
										<td class="cell_data_03 td-paymentLine2">REPORTER</td>
										<td class="cell_data_03 td-paymentLine2">G.MGR</td>
										<td class="cell_data_03 td-paymentLine2">DIR</td>
										<td class="cell_data_03 td-paymentLine2">G.DIR</td>
									</tr>
									<tr>
										<td class="cell_data_02 td-paymentLine3">&nbsp;</td>
										<td class="cell_data_01 td-paymentLine3">&nbsp;</td>
										<td class="cell_data_01 td-paymentLine3">&nbsp;</td>
										<td class="cell_data_01 td-paymentLine3">&nbsp;</td>
									</tr>
								</table>
							</td>
						</tr>
						<tr height="16">
							<td>&nbsp;COM_CD</td>
							<td>$!data.comCd</td>
						</tr>
						<tr height="16">
							<td>&nbsp;COM_NM</td>
							<td>$!data.comNm</td>
						</tr>
						<tr height="16">
							<td>&nbsp;COM_SUB_NM</td>
							<td>$!data.comSubNm</td>
						</tr>
						<tr height="16">
							<td>&nbsp;USE_YN</td>
							<td>$!data.useYn</td>
						</tr>
					</tbody>
				</table>
				<hr></hr>
				
				<div style="padding:10px; height:100%;">
					<div>
						<p>
							$!data.tmpRptRemarkEscape
						</p>
					</div>
					<table width="100%" cellpadding="0" cellspacing="0" border="0">
						<tr>
							<td>1. Enforcement</td>
							<td style="text-align:right;">(VND, V.A.T.Excluded)</td>
						</tr>
					</table>
					<table width="100%" border="0" cellspacing="0" cellpadding="4 2" class="tableRound tb-efm">
						<tbody>
							<tr>
								<th class="cell_data_00" align="center" width="">Request Type</th>
								<td class="cell_data_03" align="center">M/T Purchasing</td>
								<th class="cell_data_03" align="center" width="">Supplier</th>
								<td class="cell_data_03" align="center" width=""></td>
							</tr>
						</tbody>
					</table>
			
					<table width="100%" cellpadding="0" cellspacing="0" border="0" style="margin-top:30px;">
						<tr>
							<td>2. Contents </td>
							<td style="text-align:right;">(VND, V.A.T.Excluded)</td>
						</tr>
					</table>
					
					<table width="100%" border="0" cellspacing="0" cellpadding="4 2" class="tableRound">
						<tbody>
							<tr>
								<th class="cell_data_00" align="center" width="">No.</th>
								<th class="cell_data_03" align="center" width="">COM_CD</th>
								<th class="cell_data_03" align="center" width="">DTL_CD</th>								
								<th class="cell_data_03" align="center" width="">DTL_NM</th>
								<th class="cell_data_03" align="center" width="200">DTL_SUB_NM</th>
								<th class="cell_data_03" align="center" width="">EXTENT01</th>
								<th class="cell_data_03" align="center" width="">USE_YN</th>
								<th class="cell_data_03" align="center" width="">SORT_NO</th>
							</tr>
							#set( $i = 1 )
							#foreach( $!detail in $!listData )
								<tr>
									<td class="cell_data_02" align="center">$!i</td>
									<td class="cell_data_01" align="center">$!detail.comCd</td>
									<td class="cell_data_01" align="center">$!detail.dtlCd</td>
									<td class="cell_data_01" align="left" style="font-size:10px;">$!detail.dtlNm</td>
									<td class="cell_data_01" align="left" style="font-size:10px;">$!detail.dtlSubNm</td>
									<td class="cell_data_01" align="center">$!detail.extent01</td>
									<td class="cell_data_01" align="center">$!detail.useYn</td>
									<td class="cell_data_01" align="right" width="">$number.format("integer", $!detail.sortNo)</td>									
								</tr>
								#set( $i = $i + 1 )
							#end #* FOREACH END *#
						</tbody>
					</table>
				</div>
			</td>
		</tr>
	</table>
</div>
</body>
</html>
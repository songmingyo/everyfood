<html>
<head>
<style type="text/css">

body {
	font-size: 9pt;
	line-height: 300%;
	font-weight: normal;
	font-variant: normal;
}

th {
	background-color: #f5f5f5;
	font-weight: bold;
}

td {
	font-size: 10pt;
	color: 000000;
	line-height: 17pt;
}

.tableround {
	border-top-width: 1;
	border-right-width: 1;
	border-bottom-width: 1;
	border-left-width: 1;
	border-color: 000000;
	border-style: solid;
}

.cell_data_00 {
	font-size: 9pt;
	padding-left: 2px;
	
	background-repeat: repeat;
	border-top-width: 1;
	border-right-width: 0.99;
	border-bottom-width: 0.99;
	border-left-width: 1;
	border-color: 000000;
	border-style: solid;
	line-height: 17px;
}

.cell_data_02 {
	font-size: 9pt;
	padding-left: 2px;
	
	background-repeat: repeat;
	border-top-width: 0.01;
	border-right-width: 0.99;
	border-bottom-width: 1;
	border-left-width: 1;
	border-color: 000000;
	border-style: solid;
	line-height: 14px;
}

.cell_data_01 {
	font-size: 9pt;
	padding-left: 2px;
	
	background-repeat: repeat;
	border-top-width: 0.01;
	border-right-width: 1;
	border-bottom-width: 1;
	border-left-width: 0.01;
	border-color: 000000;
	border-style: solid;
	line-height: 17px;
}


.cell_data_03 {
	font-size: 9pt !important;
	padding-left: 2px;
	
	background-repeat: repeat;
	border-top-width: 1;
	border-right-width: 1;
	border-bottom-width: 0.99;
	border-left-width: 0.01;
	border-color: 000000;
	border-style: solid;
	line-height: 17px;
}

.header td {
	font-size: 10px;
	line-height: 15px;
}

</style>
</head>
<body style="font-family: igothic;" >
<div id="content" align="center" >

	<table width="100%" border="0" cellspacing="0" cellpadding="0" class="header">
		<tbody>
			<tr height="16">
				<td>&nbsp;COM_CD</td>
				<td>$!Data.MSG_CONTENTS_HEAD.comCd</td>
			</tr>
			<tr height="16">
				<td>&nbsp;COM_SUB_NM</td>
				<td>$!Data.MSG_CONTENTS_HEAD.comSubNm</td>
			</tr>
			<tr height="16">
				<td>&nbsp;USE_YN</td>
				<td>$!Data.MSG_CONTENTS_HEAD.useYn</td>
			</tr>
		</tbody>
	</table>

	<hr></hr>	
	
	<table width="100%" border="1" cellspacing="0" cellpadding="4 2" class="tableRound">
		<tbody>
			<tr>
				<th class="cell_data_00" align="center" width="">No.</th>
				<th class="cell_data_03" align="center" width="">COM_CD</th>
				<th class="cell_data_03" align="center" width="">DTL_CD</th>								
				<th class="cell_data_03" align="center" width="">DTL_NM</th>
				<th class="cell_data_03" align="center" width="200">DTL_SUB_NM</th>
				<th class="cell_data_03" align="center" width="">EXTENT01</th>
				<th class="cell_data_03" align="center" width="">USE_YN</th>
				<th class="cell_data_03" align="center" width="">SORT_NO</th>
			</tr>
			#set( $i = 1 )
			#foreach( $!detail in $!Data.MSG_CONTENTS_BODY )
				<tr>
					<td class="cell_data_02" align="center">$!i</td>
					<td class="cell_data_01" align="center">$!detail.comCd</td>
					<td class="cell_data_01" align="center">$!detail.dtlCd</td>
					<td class="cell_data_01" align="left" style="font-size:10px;">$!detail.dtlNm</td>
					<td class="cell_data_01" align="left" style="font-size:10px;">$!detail.dtlSubNm</td>
					<td class="cell_data_01" align="center">$!detail.extent01</td>
					<td class="cell_data_01" align="center">$!detail.useYn</td>
					<td class="cell_data_01" align="right" width="">$number.format("integer", $!detail.sortNo)</td>				
				</tr>
				#set( $i = $i + 1 )
			#end #* FOREACH END *#
		</tbody>
	</table>
</div>
</body>
</html>